{
    "name": "cmath10/mapper-bundle",
    "description": "Symfony bundle for cmath10/mapper",
    "keywords": ["mapper", "automapper", "symfony", "symfony5"],
    "type": "symfony-bundle",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "license": "MIT",
    "homepage": "https://github.com/cmath10/mapper-bundle",
    "require": {
        "php": ">=7.4",
        "cmath10/mapper": "^1.0",
        "symfony/framework-bundle": "^5.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/dependency-injection": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/translation": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "cmath10\\MapperBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cmath10\\MapperBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@cs"
        ],
        "cs": "php-cs-fixer fix -v --using-cache=no --dry-run",
        "cs-fix": "php-cs-fixer fix -v --using-cache=no"
    },
    "config": {
        "sort-packages": true
    }
}
